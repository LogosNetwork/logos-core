{
  "commands" : [
    "LOGS_ROOT=/home/ubuntu/logs-efs",
    "if [ -z $(ls -A ${LOGS_ROOT}) ]; then",
    "    N_INSPECTION=00",
    "else",
    "    LIST=$(ls ${LOGS_ROOT} | cut -d'_' -f 2)",
    "    f () {",
    "        eval echo \\${$#} | sed -e 's/^0*//'",
    "    }",
    "    N_INSPECTION=$(printf %02d $(($(f $LIST) + 0)))",
    "fi",
    "INSPECTION_DIR=${LOGS_ROOT}/inspection_${N_INSPECTION}",
    "echo \"N_INSPECTION: ${N_INSPECTION}\"",
    "# get private IP address & make corresponding dir (ALL NODES)",
    "PRIVATE_IP=$(curl http://169.254.169.254/latest/meta-data/local-ipv4)",
    "LOGS_DIR=${INSPECTION_DIR}/${PRIVATE_IP}",
    "# move logs from LogosTest dir to EFS (erasing old logs)",
    "cp -r /root/LogosTest/log/ ${LOGS_DIR}",
    "rm -rf /root/LogosTest/log/*"
  ],
  "executionTimeout":["3600"]
}