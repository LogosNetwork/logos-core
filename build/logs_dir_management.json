{
  "commands" : [
    "LOGS_ROOT=/home/ubuntu/logs-efs",
    "if [ -z $(ls -A ${LOGS_ROOT}) ]; then",
    "    N_INSPECTION=00",
    "else",
    "    LIST=$(ls ${LOGS_ROOT} | cut -d'_' -f 2)",
    "    f () {",
    "        eval echo \\${$#} | sed -e 's/^0*//'",
    "    }",
    "    N_INSPECTION=$(printf %02d $(($(f $LIST) + 1)))",
    "fi",
    "INSPECTION_DIR=${LOGS_ROOT}/inspection_${N_INSPECTION}",
    "mkdir -p ${INSPECTION_DIR}"
  ],
  "executionTimeout":["3600"]
}